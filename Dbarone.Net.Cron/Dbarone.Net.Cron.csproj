<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <PackageId>Dbarone.Net.Cron</PackageId>
    <Version>1.0.0</Version>
    <Authors>David Barone</Authors>
    <Company>Dbarone.com</Company>
    <Description>A simple .NET cron library for working with crontab strings.</Description>
    <RepositoryUrl>https://github.com/davidbarone/Dbarone.Net.Cron</RepositoryUrl>
    <PackageProjectUrl>https://github.com/davidbarone/Dbarone.Net.Cron</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Dbarone.Net.Extensions.Reflection" Version="1.0.0" />
    <PackageReference Include="Dbarone.Net.Extensions.String" Version="1.0.1" />
    <None Include="..\README.md" Pack="true" PackagePath="\"/>
  </ItemGroup>

</Project>
